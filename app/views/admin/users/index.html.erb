<center><h3><u>User</u></h3></center>
<%= link_to "Add new User", new_admin_user_path %>
<div class="col-md-3 pull-right">
  <%= form_tag admin_users_path, method: "get", class: "", id: "search-form", role: "search" do %>
  <div class="input-group">
    <%= text_field_tag :search, params[:search], class: "form-control", placeholder: "Search Video" %>
       <div class="input-group-btn">
          <button type="submit" class="btn btn-default">
             <i class="fa fa-search fa-lg"></i>
          </button>
       </div>            
  </div>
<% end %>
</div>
<table class="table">
  <thead>
    <th>Name</th>
    <th>Email</th>
    <th>Created at</th>
    <th>Role</th>    
    <th>Action</th>
  </thead>
  <tbody>
    <%= render @users %>    
  </tbody>
</table>
<%= will_paginate @users %>
