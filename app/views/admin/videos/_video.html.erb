<tr>
  <td>
    <%= video.title %>
  </td>
  <td>
    <%= truncate video.description, length: 20 %>
  </td>
  <td>
    <%= video.tutorial.title %>
  </td>
  <td>
    <%= video.duration %>
  </td>
  <td>
    <%= link_to "detail", admin_video_path(video) %>
  </td>
  <td>
    <%= link_to "edit", edit_admin_video_path(video) %>
    | <%= link_to "delete", admin_video_path(video), data: {method: :delete, confirm: "Are you sure?"} %>
  </td>
</tr>
