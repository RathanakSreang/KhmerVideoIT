<div class="row">
  <div class="col-md-4 col-md-offset-4 well">
    <%= link_to "Back", :back %>
    <center><h3><u><%= @tag.name %></u></h3></center>    
    <%= link_to "edit", edit_admin_tag_path(@tag), class: "btn btn-primary", "data-no-turbolink" => true %>
    <%= link_to "delete", admin_tag_path(@tag), data: {method: :delete, confirm: "Are you sure?"}, class: "btn btn-danger" %>
  </div>
</div>
